https://practice.geeksforgeeks.org/problems/minimum-element-in-bst/1/#


int minValue(Node* root) {
    if(root==NULL){
        return -1;
    }
   while(root->left!=NULL)
   {
       root=root->left;
   }
   return root->data;// Code here
}


int maxValue(Node* root) {
    if(root==NULL){
        return -1;
    }
   while(root->right!=NULL)
   {
       root=root->right;
   }
   return root->data;// Code here
}

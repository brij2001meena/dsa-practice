https://practice.geeksforgeeks.org/problems/search-a-node-in-bst/1/#

bool search(Node* root, int x) {
    if(root==NULL)
    {
        return false;
    }
    else if(root->data==x)
    {
        return true;
    }
    else{
        if(root->data >x)
        {
            return search(root->left , x);
        }
        else{
            return search(root->right , x);
        }
    }// Your code here
}
